<text resp_record_msg>
/ items = ("Response recorded.")
/ position = (50%, 50%)
/ fontstyle = ("Gill Sans MT", 4.25%, true, false, false, false, 5, 1)
/ txcolor = lime 
</text>

<text fixation>
/ items = ("+")
/ position = (50%, 50%)
/ fontstyle = ("Gill Sans MT", 9.25%, true, false, false, false, 5, 1)
</text>

<shape blackbox1>
/ shape = rectangle
/ color = black
/ position = (50%, 50%)
/ size = (10%, 10%)
</shape>

<shape blackbox2>
/ shape = rectangle
/ color = black
/ position = (50%, 50%)
/ size = (100%, 85%)
</shape>

<trial gridcompare_trial>
/ validresponse = (38, 50)
/ stimulustimes = [1 = fixation; 501 = blackbox1, gridface1, gridface2, gridface3, gridface4; 2501 = blackbox2; 2601 = gridface5, gridface6, gridface7, gridface8, moreorless_prompt]
/ posttrialpause = 500
/ responsemessage = (38, resp_record_msg, 0)
/ responsemessage = (50, resp_record_msg, 0)
/ ontrialend = [
	values.avg_grid1 = ((values.gridface1_code + values.gridface2_code + values.gridface3_code + values.gridface4_code)/4)
]
/ ontrialend = [
	values.avg_grid2 = ((values.gridface5_code + values.gridface6_code + values.gridface7_code + values.gridface8_code)/4)
]
/ ontrialend = [
	values.variance_grid1 = (pow((values.gridface1_code - values.avg_grid1), 2) + pow((values.gridface2_code - values.avg_grid1), 2) + pow((values.gridface3_code - values.avg_grid1), 2) + pow((values.gridface4_code - values.avg_grid1), 2))/3
]
/ ontrialend = [
	values.variance_grid2 = (pow((values.gridface5_code - values.avg_grid2), 2) + pow((values.gridface6_code - values.avg_grid2), 2) + pow((values.gridface7_code - values.avg_grid2), 2) + pow((values.gridface8_code - values.avg_grid2), 2))/3
]
/ ontrialend = [
	if (values.variance_grid1 == values.variance_grid2) {
		values.trial_retry = 1;
		values.nTrials += 1;
	}
	else {
		values.trial_retry = 0;
	}
]
/ ontrialend = [
	list.id_selector.reset()
]
</trial>

// Present trials (just 1 block of 100 trials each):

<block main_instructions>
/ bgstim = (esc_text_header)
/ trials = [1 = sequence(main_instructions1, main_instructions2, main_instructions3, main_instructions4)]
</block>

<block gridcompare_block>
/ bgstim = (esc_text_header)
/ trials = [1-1000 = gridcompare_trial]
/ stop = [block.gridcompare_block.trialcount >= values.nTrials]
</block>

