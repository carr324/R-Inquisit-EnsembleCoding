/ select = noreplace
/ position = (50%, 67.5%)
/ size = (40%, 60%)
</picture>

<text survey_text_header>
/ items = ("Use your mouse to click on the box for your answer (once you click, you will be automatically advanced to the next question).")
/ fontstyle = ("Gill Sans MT", 2.36%, true, false, false, false, 5, 1)
/ position = (50%, 10%)
/ hjustify = center
/ valign = top
/ txcolor = blue
</text>

<text trust_question>
/ items = ("How trustworthy do you find this person?")
/ fontstyle = ("Gill Sans MT", 3%, true, false, false, false, 5, 1)
/ position = (50%, 20%)
/ hjustify = center
/ valign = top
/ txcolor = black
</text>

<likert trust_scale_bf>
/ anchors=[1="Not at all"; 5="Neutral"; 9="Very much"]
/ anchorwidth = 9%
/ fontstyle = ("Gill Sans MT", 2.5%, false, false, false, false, 5, 1)
/ stimulusframes = [1 = sequence(trust_question, esc_text_header, survey_text_header, bf_pic)]
